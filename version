	<script type="text/javascript">
		var saveMostVisitedPageListTimes = 0;

		function resetTilePagePaddingTop() {
			var winHeight = window.innerHeight,
				winWidth = window.innerWidth,
				navPagePaddingTop = appsPagePaddingTop = '100px',
				navLinkWrapMarginTop = navWeatherWrapMarginTop = '65px';

			if (winHeight <= 615) {
				navPagePaddingTop = appsPagePaddingTop = '40px';
				navLinkWrapMarginTop = navWeatherWrapMarginTop = '15px';
			} else if (winHeight <= 900) {
				navPagePaddingTop = appsPagePaddingTop = '60px';
				if (screen.availWidth == 1366) {
					navPagePaddingTop = appsPagePaddingTop = '40px';
				}
			}
			if (document.querySelectorAll('.nav-page')[0])
				document.querySelectorAll('.nav-page')[0].lastChild.style.paddingTop = navPagePaddingTop;
			if (document.querySelectorAll('.nav_weather_wrap')[0])
				document.querySelectorAll('.nav_weather_wrap')[0].style.marginTop = navWeatherWrapMarginTop;
			if (document.querySelectorAll('.nav_link_wrap')[0])
				document.querySelectorAll('.nav_link_wrap')[0].style.marginTop = navLinkWrapMarginTop;
			try {
				document.querySelectorAll('.apps-page')[0].lastChild.style.paddingTop = appsPagePaddingTop;
			} catch (e) {}
		}
	</script>
