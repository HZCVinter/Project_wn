<script type="text/javascript">
	(function () {
		document.addEventListener('click', function (e) {
			var targetNode = e.target;
			var searchSwith = $("#search-switch")[0];
			if (targetNode.id != 'search-switch' && !searchSwith.contains(targetNode) && targetNode.parentNode
				.id !=
				'search-menu') {
				$('#search-menu').removeClass("toggle-display");
				document.querySelector(".search-arr >span").classList.remove("toggle-triangle");
			}

			var add_dot_con = $("#add-dot-container")[0];
			var add_dot_btn = $("#dot_add")[0];
			if (targetNode.id != "add-dot-container" && !add_dot_con.contains(targetNode) && targetNode.id !=
				"dot_add" &&
				(!add_dot_btn || (add_dot_btn && !add_dot_btn.contains(targetNode)))) {
				add_dot_con.style.display = "none";
			}
			if (targetNode.classList.contains("page-switcher")) {
				chrome.send("clearCacheNTP", ["card_change"]);
			}
		});

		var $user_add_container = false;
		document.addEventListener("keydown", function (e) {
			var getID = e.target.id;
			if ($user_add_container) {
				$user_add_container = false;
			} else {
				if (e.keyCode == 9) {
					e.preventDefault();
					e.stopPropagation();
					return false;
				}
			}
		});

		$("#user-add-container").on("keydown", function (e) {
			$user_add_container = true;
		});

		window.onload = function (e) {
			setTimeout(function () {
				chrome.send("hiddeCacheNTP");
			}, 200);
		};
		window.onresize = function (e) {
			chrome.send("clearCacheNTP", ["resize"]);
		};
	})();
	document.addEventListener('DOMContentLoaded', resetTilePagePaddingTop);
</script>
